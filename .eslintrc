{
  "extends": "airbnb",
  "env": {
    "node": false,
    "browser": true,
    "es6": true,
    "jest": true
  },
  parser: "babel-eslint",
  "rules": {
    "no-extra-parens": 0,
    "comma-dangle": [
      "error",
      "never"
    ],
    "react/jsx-uses-vars": 1,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0,
    "react/self-closing-comp": ["error", {
      "component": true,
      "html": true
    }],
    "jsx-a11y/no-autofocus": 'off'
  }
}
